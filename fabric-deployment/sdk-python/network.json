{
  "name": "sample-network",
  "description": "Sample network contains 4 peers (2 orgs), 1 orderer and 2 cas for Python SDK testing",
  "version": "0.1",
  "client": {
    "organization": "OrgBHospital",
    "credentialStore": {
      "path": "/tmp/hfc-kvs",
      "cryptoStore": {
        "path": "/tmp/hfc-cvs"
      },
      "wallet": "wallet-name"
    }
  },
  "organizations": {
    "orderer1.trustchain.com": {
      "mspid": "OrdererMSP",
      "orderers": [
        "orderer1.trustchain.com"
      ],
      "certificateAuthorities": [
        "ca-orderer"
      ],
      "users": {
        "Admin": {
          "cert": "./crypto-config/ordererOrganizations/trustchain.com/users/Admin@trustchain.com/msp/signcerts/Admin@trustchain.com-cert.pem",
          "private_key": "./crypto-config/ordererOrganizations/trustchain.com/users/Admin@trustchain.com/msp/keystore/a7502f3d9707d9c2ca936361ca8bb6ed6fa5c636457d7bce8f351cf8bfda7cf4_sk"
        }
      }
    },
    "peer0.AHospital.trustchain.com": {
      "mspid": "AHospitalMSP",
      "peers": [
        "peer0.AHospital.trustchain.com"
      ],
      "certificateAuthorities": [
        "ca-orgA"
      ],
      "users": {
        "Admin": {
          "cert": "./crypto-config/peerOrganizations/AHospital.trustchain.com/users/Admin@AHospital.trustchain.com/msp/signcerts/Admin@AHospital.trustchain.com-cert.pem",
          "private_key": "./crypto-config/peerOrganizations/AHospital.trustchain.com/users/Admin@AHospital.trustchain.com/msp/keystore/28b0f120c7562db68683ff37c246d84a64f84da00157765fe909c1d203d783cf_sk"
        }
      }
    },
    "peer0.BHospital.trustchain.com": {
      "mspid": "BHospitalMSP",
      "peers": [
        "peer0.BHospital.trustchain.com"
      ],
      "certificateAuthorities": [
        "ca-orgB"
      ],
      "users": {
        "Admin": {
          "cert": "./crypto-config/peerOrganizations/BHospital.trustchain.com/users/Admin@BHospital.trustchain.com/msp/signcerts/Admin@BHospital.trustchain.com-cert.pem",
          "private_key":"./crypto-config/peerOrganizations/BHospital.trustchain.com/users/Admin@BHospital.trustchain.com/msp/keystore/75214e9125760153e6c131983549207b7d26a2f17cc96ceeb50329abc402f777_sk"
        }
      }
    }
  },
  "orderers": {
    "orderer1.trustchain.com": {
      "url": "192.168.8.112:7050",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "orderer1.trustchain.com",
        "grpc-max-send-message-length": 15
      },
      "tlsCACerts": {
        "path": "./crypto-config/ordererOrganizations/trustchain.com/tlsca/tlsca.trustchain.com-cert.pem"
      }
    }
  },
  "peers": {
    " peer0.AHospital.trustchain.com": {
      "url": "192.168.8.225:7051",
      "eventUrl": "192.168.8.225:7051",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "peer0.AHospital.trustchain.com",
        "grpc.http2.keepalive_time": 15
      },
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/AHospital.trustchain.com/tlsca/tlsca.AHospital.trustchain.com-cert.pem"
      }
    },
    "peer0.BHospital.trustchain.com": {
      "url": "192.168.8.139:7051",
      "eventUrl": "192.168.8.139:7051",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "peer0.BHospital.trustchain.com"
      },
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/BHospital.trustchain.com/tlsca/tlsca.BHospital.trustchain.com-cert.pem"
      }
    }
  },
  "certificateAuthorities": {
    "ca-orgA": {
      "url": "https://localhost:7054",
      "grpcOptions": {
        "verify": true
      },
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/AHospital.trustchain.com/ca/ca.AHospital.trustchain.com-cert.pem"
      },
      "registrar": [
        {
          "enrollId": "admin",
          "enrollSecret": "adminpw"
        }
      ]
    },
    "ca-orgB": {
      "url": "https://localhost:8054",
      "grpcOptions": {
        "verify": true
      },
      "tlsCACerts": {
        "path": "./crypto-config/peerOrganizations/BHospital.trustchain.com/ca/ca.BHospital.trustchain.com-cert.pem"
      },
      "registrar": [
        {
          "enrollId": "admin",
          "enrollSecret": "adminpw"
        }
      ]
    }
  }
}
